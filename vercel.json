{
    "regions": [
      "sfo1",
      "bom1",
      "iad1"
    ],
    "builds": [{
        "src": "blogapp/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.11" 
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "blogapp/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}
